# AWS
awsprof() {
    if [ -z $1 ]; then
        echo $AWS_DEFAULT_PROFILE;
    else
        export AWS_DEFAULT_PROFILE=$1;
    fi
}

# Pulse Secure
alias pulseon="launchctl load -w /Library/LaunchAgents/net.pulsesecure.pulsetray.plist"
alias pulseoff="launchctl unload -w /Library/LaunchAgents/net.pulsesecure.pulsetray.plist"
